{
  "name": "@gov.au/datavizkit",
  "version": "0.0.11-development",
  "description": "A fully featured data visualisation toolset built on top of Highcharts and React.",
  "keywords": [
    "highcharts",
    "dataviz"
  ],
  "license": "MIT",
  "author": "Digital Transformation Agency",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/govau/datavizkit.git"
  },
  "main": "index.js",
  "module": "lib/Datavizkit.js",
  "bugs": {
    "url": "https://github.com/govau/datavizkit/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://datavizkit.surge.sh/",
  "dependencies": {
    "highcharts": "^5.0.9",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.5.4",
    "recompose": "^0.23.1",
    "styled-components": "^1.4.4"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-app": "^2.2.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "commitizen": "^2.9.6",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "cz-conventional-changelog": "^2.0.0",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "eslint": "3.16.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "husky": "^0.13.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "react-dev-utils": "^0.5.2",
    "react-styleguidist": "^5.0.2",
    "semantic-release": "^6.3.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "scripts": {
    "clean:lib": "rm -rf lib",
    "postmerge": "yarn install",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:lib": "yarn clean:lib && babel src/ -d lib --ignore=index.js --source-maps",
    "test": "node scripts/test.js --env=jsdom",
    "test:ci": "npm run test -- --coverage --runInBand",
    "docs-server": "styleguidist server",
    "docs-build": "styleguidist build",
    "docs-deploy": "npm run docs-build && surge -p $npm_package_config_styleguideDir -d https://datavizkit.surge.sh",
    "commit": "git-cz",
    "hard-release": "yarn run build:lib && npm publish",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "styleguideDir": "docs/.out"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "directories": {
    "doc": "docs"
  }
}
